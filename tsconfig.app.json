{
  "compilerOptions": {
    "composite": true,
  "target": "ES2020",
    "useDefineForClassFields": true,
  "lib": ["ES2020", "ES2021", "ES2022", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "skipLibCheck": true,
    "moduleResolution": "Bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
  "types": ["vite/client", "node"]
  },
  "include": ["src"]
}
